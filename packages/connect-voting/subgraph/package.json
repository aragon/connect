{
  "name": "@1hive/subgraph-voting",
  "version": "0.0.1",
  "scripts": {
    "codegen": "rm -rf generated; graph codegen",
    "build": "rm -rf build; graph build",
    "manifest-mainnet": "scripts/build-manifest.sh mainnet",
    "manifest-mainnet-staging": "env STAGING=true scripts/build-manifest.sh mainnet",
    "manifest-goerli": "scripts/build-manifest.sh goerli",
    "manifest-goerli-staging": "env STAGING=true scripts/build-manifest.sh goerli",
    "manifest-gnosis": "scripts/build-manifest.sh gnosis",
    "deploy-mainnet": "scripts/deploy.sh 1hive voting mainnet",
    "deploy-mainnet-staging": "env STAGING=true scripts/deploy.sh 1hive voting mainnet",
    "deploy-goerli": "scripts/deploy.sh 1hive voting goerli",
    "deploy-goerli-staging": "env STAGING=true scripts/deploy.sh 1hive voting goerli",
    "deploy-gnosis": "scripts/deploy.sh 1hive voting gnosis"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.39.0",
    "@graphprotocol/graph-ts": "^0.29.3",
