    "compile": "tsc",
    "lint": "eslint --ext .ts ./src"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
