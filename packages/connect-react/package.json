    "eslint": "^7.2.0",
    "is-ci-cli": "^2.1.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@aragon/connect": "*"
  },
  "description": "Access and interact with Aragon Organizations and their apps.",
  "keywords": [
    "ethereum",
    "javascript",
    "web3",
    "dapp",
